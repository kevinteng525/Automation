  <s1configs>
    <archiveconnections>
      <archiveconnection>
        <databaseserver>ES1All</databaseserver>
        <databasename>ES1Archive</databasename>
        <nativearchiveservers>
          <archiveserver>
            <name>ES1All</name>
            <nativearchiveserverconfiguration>
              <archive>
                <enable>true</enable>
                <messagecenterlocation>\\ES1All\Share\MC</messagecenterlocation>
                <volumeidletime>172800</volumeidletime>
              </archive>
              <index>
                <enable>true</enable>
                <archiveservers>
                  <archiveserver>ES1ALL</archiveserver>
                </archiveservers>
                <componentlimitperindexaction_add>4</componentlimitperindexaction_add>
                <componentlimitperindexaction_delete>2</componentlimitperindexaction_delete>
                <componentlimitperindexaction_repair>2</componentlimitperindexaction_repair>
                <componentlimitperindexaction_update>4</componentlimitperindexaction_update>
                <runthreshod>45</runthreshod>
              </index>
              <search>
                <enable>true</enable>
                <memoryallocated>50</memoryallocated>
              </search>
              <retrieval>
                <enable>true</enable>
              </retrieval>
            </nativearchiveserverconfiguration>
          </archiveserver>
        </nativearchiveservers>
      </archiveconnection>
    </archiveconnections>
    <workers>
      <worker>
        <name>ES1All</name>
        <jobsenabled></jobsenabled>
      </worker>      
    </workers>
    <sharepoint>Hello World</sharepoint>
